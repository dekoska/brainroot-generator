{
  "routes": [
    {
      "src": "/python/(.*)",
      "dest": "python/app/main.py"
    },
    {
      "src": "/frontend/main",
      "dest": "/frontend/main.html"
    },
    {
      "src": "/frontend/login",
      "dest": "/frontend/login.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/server.js"
    },
    {
      "src": "/js/(.*)",
      "dest": "/frontend/js/$1"
    }
  ]
}


